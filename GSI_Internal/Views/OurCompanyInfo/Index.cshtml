@model IEnumerable<OurCompanyInfo_VM>
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
<section class="content-header">
    <div class="container-fluid">
        <div class="row mb-2">
            <div class="col-sm-6">
            </div>
            <div class="col-sm-6">
                <ol class="breadcrumb float-sm-right">
                    <li class="breadcrumb-item"><a asp-action="Index" asp-controller="Dashboard">Dashboard</a></li>
                    <li class="breadcrumb-item active">Our Company </li>
                </ol>
            </div>
        </div>
    </div><!-- /.container-fluid -->
</section>


<a asp-action="Create" asp-controller="OurCompanyInfo" class="btn btn-primary">Add </a>
<br />
<br />
<table class="table">
    <thead>
    <tr>
        <th>About US Englis</th>
        <th>About US Arabic</th>
        <th> Our Mission Arabic</th>
        <th> Our Vision Englis</th>
        <th>Edit</th>
       
    </tr>
    </thead>
    <tbody>

    @foreach (var item in Model)
    {
           
        <tr>
           
            <td>@item.AboutUS_Englis</td>
            <td>@item.AboutUS_Arabic</td>
            <td>@item.OurMission_Arabic</td>
            <td>@item.OurVision_Englis</td>

            <td><a href="/OurCompanyInfo/Edit/1" class="btn btn-warning">Edit</a></td>
            
        </tr>
    }
    </tbody>
</table>