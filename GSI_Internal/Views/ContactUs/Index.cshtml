@model IEnumerable<ContactUsVM>
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
<section class="content-header">
    <div class="container-fluid">
        <div class="row mb-2">
            <div class="col-sm-6">
            </div>
            <div class="col-sm-6">
                <ol class="breadcrumb float-sm-right">
                    <li class="breadcrumb-item"><a asp-action="Index" asp-controller="Dashboard">Dashboard</a></li>
                    <li class="breadcrumb-item active">Contact Us Details /TermsAndConditions  </li>
                </ol>
            </div>
        </div>
    </div><!-- /.container-fluid -->
</section>


<a asp-action="Create" asp-controller="ContactUs" class="btn btn-primary">Add Or Edit </a>
<br />
<br />
<table class="table">
    <thead>
        <tr>
            <th>FaceBookLink</th>
            <th>PhoneNumber</th>
            <th> WhatsAppNumber</th>
            <th> Email</th>
            <th> LocationAr</th>

        </tr>
    </thead>
    <tbody>

        @foreach (var item in Model)
        {

            <tr>

                <td>@item.FaceBookLink</td>
                <td>@item.PhoneNumber</td>
                <td>@item.WhatsAppNumber</td>
                <td>@item.Email</td>
                <td>@item.LocationAr</td>
                @item.TermsAndConditions
               



            </tr>
        }
    </tbody>
</table>