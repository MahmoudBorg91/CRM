@model ApplicationTransaction_RequestVM
@{
    ViewData["Title"] = "CheckRequestApplicationToOpen";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<form asp-action="CheckRequestApplicationToOpen" asp-controller="Dashboard" enctype="multipart/form-data" class="form-horizontal">

    <div class="row">
        <div class="col-12 col-sm-12">
            <div class="card card-primary card-tabs">
                <div class="card-header p-0 pt-1">
                    <ul class="nav nav-tabs" id="custom-tabs-one-tab" role="tablist">
                        <li class="nav-item">
                            <a class="nav-link active" id="custom-tabs-one-ClientCard-tab" data-toggle="pill" href="#custom-tabs-one-ClientCard" role="tab" aria-controls="custom-tabs-one-ClientCard" aria-selected="true">Client Card</a>
                        </li>
                       
                    </ul>
                </div>
                <div class="card-body">
                    <div class="tab-content" id="custom-tabs-one-tabContent">
                        <div class="tab-pane fade show active" id="custom-tabs-one-ClientCard" role="tabpanel" aria-labelledby="custom-tabs-one-ClientCard-tab">
                            <div class="card-body">
                                <div class="row-md-12">
                                    <div class="row">
                                        <div class="col-md-6">
                                            <div class="form-group">
                                                <label>Request Number:</label>
                                                <td><input type="text" asp-for="ID" class="form-control" id="ID" readonly></td>
                                                <input hidden  asp-for="IsProsessByUser"/>
                                                <input  asp-for="UserProsessID"/>
                                                <input  asp-for="ProsessID" />
                                            </div>

                                        </div>

                                        <div class="col-md-6">
                                            <div class="form-group">
                                                <label>Item Name :</label>
                                                <td><input type="text" asp-for="TransiactionItem_Name" class="form-control" id="ID" readonly></td>
                                            </div>

                                        </div>
                                    </div>

                                  
                                    
                                </div>

                                <div class="row-md-12">
                                    <div class="row">
                                        <div class="col-md-6">
                                            <div class="form-group">
                                                <input type="submit" value="Start Processing"/>
                                            </div>

                                        </div>

                                      
                                    </div>



                                </div>




                            </div>
                        </div>
                        
                    </div>
                </div>
                <!-- /.card -->
            </div>
        </div>
    </div>





</form>
@section Scripts{
    <script>
        $(document).ready(function () {
         

        });
          // DropzoneJS Demo Code End
    </script>
}


