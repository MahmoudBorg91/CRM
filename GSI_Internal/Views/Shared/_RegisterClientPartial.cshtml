@using Microsoft.AspNetCore.Identity
@using Microsoft.AspNetCore.Mvc.TagHelpers
@using GSI_Internal.Entites

@inject SignInManager<ApplicationUser> SignInManager
@inject UserManager<ApplicationUser> UserManager

@model GSI_Internal.Areas.Identity.Pages.Account.RegisterModel.InputModel
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers

  <div id="Past" class="tab-pane">
                            <div class="sf-tabs-content">
                                <form class="aon-login-form" asp-area="Identity" asp-page="/Account/Register"     method="post">
                                    <div class="row">
                                        <div class="col-md-6">
                                            <div class="form-group">
                                                <div class="aon-inputicon-box"> 
                                                    <input asp-for="FirstName" class="form-control  sf-form-control" name="FirstName" type="text" placeholder="First Name">
                                                    <i class="aon-input-icon fa fa-user"></i>
                                                    <span asp-validation-for="FirstName" class="text-danger"></span>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-md-6">
                                            <div class="form-group">
                                                <div class="aon-inputicon-box"> 
                                                    <input asp-for="LastName" class="form-control sf-form-control" name="LastName" type="text" placeholder="Last Name">
                                                    <i class="aon-input-icon fa fa-user"></i>
                                                    <span asp-validation-for="LastName" class="text-danger"></span>
                                                </div>
                                            </div>
                                        </div>
                                        
                                        <div class="col-md-12">
                                            <div class="form-group">
                                                <div class="aon-inputicon-box"> 
                                                    <input asp-for="PhoneNumber" class="form-control sf-form-control" name="PhoneNumber" type="text" placeholder="Phone">
                                                    <i class="aon-input-icon fa fa-phone"></i>
                                                    <span asp-validation-for="PhoneNumber" class="text-danger"></span>
                                                </div>
                                            </div>
                                        </div>
                                        
                                        <div class="col-md-12">
                                            <div class="form-group">
                                                <div class="aon-inputicon-box"> 
                                                    <input asp-for="Email" class="form-control sf-form-control" name="Email" type="email" placeholder="Email">
                                                    <i class="aon-input-icon fa fa-envelope-o"></i>
                                                    <span asp-validation-for="Email" class="text-danger"></span>
                                                </div>
                                            </div>
                                        </div>
                                        
                                        <div class="col-md-12">
                                            <div class="form-group">
                                                <div class="aon-inputicon-box"> 
                                                    <input asp-for="Password" class="form-control sf-form-control" name="Password" type="password" placeholder="Password">
                                                    <i class="aon-input-icon fa fa-lock"></i>
                                                    <span asp-validation-for="Password" class="text-danger"></span>
                                                </div>
                                            </div>
                                        </div>
                                        
                                         <div class="col-md-12">
                                            <div class="form-group">
                                                <div class="aon-inputicon-box"> 
                                                    <input asp-for="ConfirmPassword" class="form-control sf-form-control" name="ConfirmPassword" type="password" placeholder="Confirm Password">
                                                    <i class="aon-input-icon fa fa-lock"></i>
                                                    <span asp-validation-for="ConfirmPassword" class="text-danger"></span>
                                                </div>
                                            </div>
                                         </div>     
                                        
                                        <div hidden="" class="col-md-12">
                                            <div class="form-group">
                                                <div class="aon-inputicon-box"> 
                                                    <input asp-for="UserType" class="form-control sf-form-control" name="UserType" value="1" type="text" placeholder="Confirm Password">
                                                    <i class="aon-input-icon fa fa-lock"></i>
                                                    <span asp-validation-for="ConfirmPassword" class="text-danger"></span>
                                                </div>
                                            </div>
                                        </div> 
                                        
                                        <div class="col-md-12">
                                            <div class="form-group sign-term-con">
                                                <div class="checkbox sf-radio-checkbox">
                                                    <input id="td33" name="abc" value="five" type="checkbox">
                                                    <label for="td33">I've read and agree with your <a href="#">Privacy Policy</a> and <a href="#">Terms & Conditions</a> </label>
                                                </div>
                                            </div>
                                        </div>  
                                        <div class="col-md-12">
                                            <button type="submit" class="site-button w-100">Submit <i class="feather-arrow-right"></i> </button>
                                        </div>                                          
                                        
                                    </div>
                                </form>
                            </div>
                        </div>